{
  "name": "ngx-bind-io",
  "version": "0.6.6",
  "description": "Directives for auto binding Input() and Output() in Angular7+ application",
  "keywords": [
    "angular",
    "ng",
    "ngx",
    "input",
    "output",
    "directives",
    "dynamic",
    "auto",
    "bind",
    "binding",
    "inheritance",
    "hierarchy",
    "nested",
    "class",
    "component",
    "extends",
    "angular2",
    "angular 2",
    "angular4",
    "angular 4",
    "angular5",
    "angular 5",
    "angular6",
    "angular 6",
    "angular7",
    "angular 7",
    "angularx",
    "angular-x",
    "ng2",
    "ng4",
    "ng5",
    "ng6",
    "ng7"
  ],
  "license": "MIT",
  "author": "EndyKaufman <admin@site15.ru>",
  "engines": {
    "node": ">=11",
    "npm": ">=6.5.0"
  },
  "bugs": {
    "url": "https://github.com/EndyKaufman/ngx-bind-io/issues"
  },
  "homepage": "https://github.com/EndyKaufman/ngx-bind-io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EndyKaufman/ngx-bind-io.git"
  },
  "maintainers": [
    {
      "name": "EndyKaufman",
      "email": "admin@site15.ru"
    }
  ],
  "scripts": {
    "config:mono": "rucken config -m dev",
    "config:prod": "rucken config -m prod",
    "prepare:mono": "rucken prepare -m dev",
    "prepare:prod": "rucken prepare -m prod",
    "lib:build-lib": "ng build ngx-bind-io --prod",
    "lib:postbuild-lib": "./node_modules/.bin/cp-cli ./CHANGELOG.md ./dist/ngx-bind-io/CHANGELOG.md",
    "lib:build": "run-s prepare:prod lib:build-lib lib:postbuild-lib",
    "app:prebuild": "ng build demo --prod",
    "app:build": "run-s format:fix prepare:mono app:prebuild",
    "lite-server": "lite-server -c bs-config.json",
    "ng": "ng",
    "serve": "ng serve",
    "start": "run-s format:fix prepare:mono serve",
    "build": "run-s app:build",
    "test": "run-s github:build",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "github:prebuild": "ng build demo --base-href /ngx-bind-io/ --prod",
    "github:build": "run-s format:fix lib:build config:prod github:prebuild github:postbuild",
    "github:postbuild": "./node_modules/.bin/cp-cli ./dist/demo/index.html ./dist/demo/404.html",
    "github:deploy": "gh-pages -d dist/demo",
    "format:fix": "prettier  --config ./.prettierrc --write \"{apps,libs}/**/*.ts\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.4",
    "@angular/cdk": "^7.3.1",
    "@angular/common": "^7.2.4",
