    "lite-server": "^2.4.0",
    "markdown-loader": "^5.0.0",
    "ng-packagr": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.3",
    "protractor": "~5.4.2",
    "ts-node": "~8.0.2",
    "tsickle": "^0.34.0",
    "tslib": "^1.9.3",
    "tslint": "~5.12.1",
